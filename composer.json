{
    "name": "web-id/flan",
    "description": "FLAN - Filter Like A Ninja",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "TIOLLIER LÃ©o",
            "email": "leo@web-id.fr"
        }
    ],
    "homepage": "https://github.com/web-id/flan",
    "keywords": ["Laravel", "Flan", "Filter", "Package"],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "~7|~8",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.0",
        "phpstan/phpstan": "^0.12.70",
        "nunomaduro/larastan": "^0.6.13"
    },
    "autoload": {
        "psr-4": {
            "WebId\\Flan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebId\\Flan\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebId\\Flan\\FlanServiceProvider"
            ],
            "aliases": {
                "Flan": "WebId\\Flan\\Facades\\Flan"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
